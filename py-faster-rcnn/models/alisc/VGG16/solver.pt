train_net: "models/alisc/VGG16/train.prototxt"
test_net: "models/alisc/VGG16/val.prototxt"
test_iter: 1000
test_interval: 500
test_initialization: false
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 1000000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_fast_rcnn"
